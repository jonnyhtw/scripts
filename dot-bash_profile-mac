#import aliases
. ~/scripts/aliases
export PS1="\d|\t|\W> "
#add scripts directory to $PATH
export PATH=~/scripts:$PATH
export PATH=~/miniconda3/bin:$PATH
export EDITOR=vim
set -o vi

#allows ctrl+O to work in mutt when opening sidebar mailbox
stty flush undef

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/Jonny/miniconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/Jonny/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/Jonny/miniconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/Jonny/miniconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<


##
# Your previous /Users/Jonny/.bash_profile file was backed up as /Users/Jonny/.bash_profile.macports-saved_2022-09-27_at_17:58:51
##

# MacPorts Installer addition on 2022-09-27_at_17:58:51: adding an appropriate PATH variable for use with MacPorts.
export PATH="/opt/local/bin:/opt/local/sbin:$PATH"
# Finished adapting your PATH environment variable for use with MacPorts.

export BROWSER='open -a "Google Chrome"'

export TERM=xterm-256color
